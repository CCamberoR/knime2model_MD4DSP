    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:Contract="https://www.example.org/contract"
    xmlns:Library="http://www.example.org/Library"
    xmlns:Workflow="https://www.example.org/workflow"
    xsi:schemaLocation="https://www.example.org/contract ../metamodel/Contract.ecore http://www.example.org/Library ../metamodel/Library.ecore https://www.example.org/workflow ../metamodel/Workflow.ecore"
    name="02 Data Cleaning_Workflow3-1">
  <dataprocessing
      xsi:type="Workflow:DataProcessing"
      in="//@dataprocessing.0/@inputPort.0/@datafield.0"
      out="//@dataprocessing.0/@outputPort.0/@datafield.0"
      name="mapping(Country)">
    <inputPort
        fileName="mapping_input_dataDictionary.csv"
        name="mapping(Country)_input_dataDictionary"
        out="//@dataprocessing.0/@outputPort.0">
      <datafield
          xsi:type="Workflow:Categorical"
          name="mapping(Country)_input_dataField"
          displayName="Country"
          out="//@dataprocessing.0/@outputPort.0/@datafield.0">
        <categoricalDef
            href="library_validation.xmi#//@dataprocessingdefinition.4/@inputPort.0/@datafielddefinition.0"/>
      </datafield>
      <dataDictionaryDefinition
          href="library_validation.xmi#//@dataprocessingdefinition.4/@inputPort.0"/>
    </inputPort>
    <outputPort
        fileName="mapping_output_dataDictionary.csv"
        name="mapping(Country)_output_dataDictionary"
        in="//@dataprocessing.0/@inputPort.0">
      <datafield
          xsi:type="Workflow:Categorical"
          name="mapping(Country)_output_dataField"
          displayName="Country"
          in="//@dataprocessing.0/@inputPort.0/@datafield.0">
        <categoricalDef
            href="library_validation.xmi#//@dataprocessingdefinition.0/@outputPort.0/@datafielddefinition.0"/>
      </datafield>
      <dataDictionaryDefinition
          href="library_validation.xmi#//@dataprocessingdefinition.4/@outputPort.0"/>
    </outputPort>
    <dataProcessingDefinition
        xsi:type="Library:Transformation"
        href="library_validation.xmi#//@dataprocessingdefinition.4"/>
    <parameter
        xsi:type="Workflow:Map"
        name="mapping(Country)_param_United States"
        inValue="United States"
        outvalue="USA"
        mapOperation="SUBSTRING">
      <mapDefinition
          href="library_validation.xmi#//@dataprocessingdefinition.4/@parameterdefinition.0"/>
    </parameter>
    <contract
        name="mapping(Country)_PRECONDITION">
      <contractDefinition
          href="library_validation.xmi#//@dataprocessingdefinition.4/@contractdefinition.1"/>
      <contract
          xsi:type="Contract:ValueRange"
          name="mapping(Country)_PRE_valueRange">
        <in
            xsi:type="Contract:DataField"
            dataField="//@dataprocessing.0/@inputPort.0/@datafield.0"
            name="mapping(Country)_PRE_input_datafield">
          <dataFieldDef
              href="library_validation.xmi#//@dataprocessingdefinition.4/@contractdefinition.1/@contractDefinition/@in.0"/>
        </in>
        <value
            xsi:type="Contract:FixValue"
            value="United States"
            subStr="true">
          <valueFieldDef
              href="library_validation.xmi#//@dataprocessingdefinition.4/@contractdefinition.1/@contractDefinition/@valuedefinition.0"/>
        </value>
        <valueRangeDef
            href="library_validation.xmi#//@dataprocessingdefinition.4/@contractdefinition.1/@contractDefinition"/>
      </contract>
    </contract>
    <contract
        name="mapping(Country)_POSTCONDITION">
      <contractDefinition
          href="library_validation.xmi#//@dataprocessingdefinition.4/@contractdefinition.0"/>
      <contract
          xsi:type="Contract:ValueRange"
          type="POSTCONDITION"
          name="mapping(Country)_POST_valueRange"
          belongOp="NOTBELONG">
        <in
            xsi:type="Contract:DataField"
            dataField="//@dataprocessing.0/@outputPort.0/@datafield.0"
            name="mapping(Country)_POST_input_datafield">
          <dataFieldDef
              href="library_validation.xmi#//@dataprocessingdefinition.4/@contractdefinition.0/@contractDefinition/@in.0"/>
        </in>
        <value
            xsi:type="Contract:FixValue"
            value="United States">
          <valueFieldDef
              href="library_validation.xmi#//@dataprocessingdefinition.4/@contractdefinition.0/@contractDefinition/@valuedefinition.0"/>
        </value>
        <valueRangeDef
            href="library_validation.xmi#//@dataprocessingdefinition.4/@contractdefinition.0/@contractDefinition"/>
      </contract>
    </contract>
    <contract
        name="mapping(Country)_INVARIANT">
      <contractDefinition
          href="library_validation.xmi#//@dataprocessingdefinition.4/@contractdefinition.2"/>
      <contract
          xsi:type="Contract:Condition"
          type="INVARIANT"
          name="mapping(Country)_INV_condition">
        <in
            xsi:type="Contract:DataField"
            dataField="//@dataprocessing.0/@inputPort.0/@datafield.0"
            name="mapping(Country)_INV_input_dataField">
          <dataFieldDef
              href="library_validation.xmi#//@dataprocessingdefinition.4/@contractdefinition.2/@contractDefinition/@in.0"/>
        </in>
        <out
            xsi:type="Contract:DataField"
            dataField="//@dataprocessing.0/@outputPort.0/@datafield.0"
            name="mapping(Country)_INV_output_dataField">
          <dataFieldDef
              href="library_validation.xmi#//@dataprocessingdefinition.4/@contractdefinition.2/@contractDefinition/@out.0"/>
        </out>
        <conditionDef
            href="library_validation.xmi#//@dataprocessingdefinition.4/@contractdefinition.2/@contractDefinition"/>
        <if
            then="//@dataprocessing.0/@contract.2/@contract/@then.0"
            name="mapping(Country)_INV_IF_United States">
          <ifDefinition
              href="library_validation.xmi#//@dataprocessingdefinition.4/@contractdefinition.2/@contractDefinition/@ifdefinition.0"/>
          <valueCondition
              xsi:type="Contract:FixValue"
              value="United States">
            <valueFieldDef
                href="library_validation.xmi#//@dataprocessingdefinition.4/@contractdefinition.2/@contractDefinition/@ifdefinition.0/@valueCondition.0"/>
          </valueCondition>
        </if>
        <then
            if="//@dataprocessing.0/@contract.2/@contract/@if.0"
            name="mapping(Country)_INV_THEN_United States">
          <thenDefinition
              href="library_validation.xmi#//@dataprocessingdefinition.4/@contractdefinition.2/@contractDefinition/@thendefinition.0"/>
          <valueResult
              xsi:type="Contract:FixValue"
              value="USA">
            <valueFieldDef
                href="library_validation.xmi#//@dataprocessingdefinition.4/@contractdefinition.2/@contractDefinition/@thendefinition.0/@valueResult"/>
          </valueResult>
        </then>
      </contract>
    </contract>
  </dataprocessing>
</Workflow:Workflow>
